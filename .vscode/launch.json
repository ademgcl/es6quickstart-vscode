{
    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/src/app.js",
            "outFiles": [
                "${workspaceRoot}/out/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Current File",
            "program": "${file}",
            "outFiles": [
                "${workspaceRoot}/out/**/*.js"
            ]
        }
    ]
}